                operationsPerSecond: `${performanceMetrics.operationsPerSecond.toFixed(0)} (target: >${targets.minOperationsPerSecond})`,
                latency: `${performanceMetrics.averageLatency.toFixed(2)}ms (target: <${targets.maxLatency}ms)`,
